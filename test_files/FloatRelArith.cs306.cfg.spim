
	.text 			# The .text assembler directive indicates
	.globl main		# The following is the code (as oppose to data)
main:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $fp, 0($sp)		# Save the frame pointer
	sub $fp, $sp, 4		# Update the frame pointer

	sub $sp, $sp, 4		#Make space for the locals
# Prologue ends


label2:
	li $f2, 2.30
	sw $f2, 4($fp)
	li $f2, 3.40
	sw $f2, 2($fp)
	lw $f2, 4($fp)
	lw $f4, 2($fp)
	lw $f2, 4($fp)
	li $t0, 0
	bne $t1, $zero, label3
 j label4

label3:
	li $f2, 1.00
	sw $f2, 6($fp)
 j label5

label4:
	li $f2, 0.00
	sw $f2, 6($fp)

label5:
	lw $f2, 6($fp)
	lw $f4, 2($fp)
	li $f6, 2.00
	li $t0, 0
	bne $t1, $zero, label6
 j label7

label6:
	li $f2, 1.00
	sw $f2, 8($fp)
 j label8

label7:
	li $f2, 0.00
	sw $f2, 8($fp)

label8:
	lw $f2, 8($fp)
	sw $f2, 0($fp)

#Epilogue Begins
	add $sp, $sp, 4
	lw $fp, 0($sp)  
	jr        $31		# Jump back to the operating system.
# Epilogue Ends

